		{
			"name": "GetKeysList",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"",
							"pm.test(\"Response time is within an acceptable range\", function () {",
							"  pm.expect(pm.response.responseTime).to.be.below(200);",
							"});",
							"",
							"",
							"pm.test(\"Presence of fields - rowId, key, language, and value\", function () {",
							"    const responseData = pm.response.json();",
							"    ",
							"    pm.expect(responseData).to.be.an('array').that.is.not.empty;",
							"    responseData.forEach(function(entry) {",
							"        pm.expect(entry).to.have.property('rowId');",
							"        pm.expect(entry).to.have.property('key');",
							"        pm.expect(entry).to.have.property('language');",
							"        pm.expect(entry).to.have.property('value');",
							"    });",
							"});",
							"",
							"",
							"pm.test(\"Key newKey1 found\", function () {",
							"    const responseData = pm.response.json();",
							"    ",
							"    pm.expect(responseData).to.be.an('array').that.is.not.empty;",
							"    ",
							"    let keyFound = false;",
							"    responseData.forEach(function(item) {",
							"        if (item.key && item.key.keyValue === \"newKey1\") {",
							"            keyFound = true;",
							"        }",
							"    });",
							"    pm.expect(keyFound).to.be.true;",
							"",
							"    let valueFound = false;",
							"    responseData.forEach(function(item) {",
							"        if (item.value === \"newValue1\") {",
							"            valueFound = true;",
							"        }",
							"    });",
							"    pm.expect(valueFound).to.be.true;",
							"",
							"",
							"});",
							"",
							"",
							"",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
